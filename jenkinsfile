pipeline {
  agent any
  stages {
    stage('checkout') {
      steps{
        echo 'Downloading markdown'
        checkout([
          $class: 'hudson.plugins.git.GitSCM',
          branches: [[name: 'ref/heads/master']],
          doGenerateSubmoduleConfigurations: false,
          externsions: [[$class: 'RelativeTargetDirectory', relativeTargetDir: 'markdown']],
          submoduleCfg: [],
          userRemoteConfigs: [[url: 'https://github.com/markdownlint/markdownlint.git']]
        ])
      }
    }
    stage('markdown') {
      steps {
        echo 'markdown..'
      }
    }
    stage('shellcheck') {
      steps {
        echo 'shellcheck..'
      }
    }
    stage('groovy') {
      steps {
        echo 'groovy..'
      }
    }
    stage('json') {
      steps {
        echo 'json..'
      }
    }
  }
}
